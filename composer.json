{
	"name": "fuzz/laravel-oauth",
	"description": "auth",
	"license": "MIT",
	"require": {
		"laravel/framework": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
		"lucadegasperi/oauth2-server-laravel": "5.2.*",
		"symfony/security-core": "3.0.*",
		"fuzz/magic-box": "1.2.x-dev as 1.2"
	},
	"require-dev": {
		"orchestra/testbench": "3.3.*",
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "0.9.*",
		"phpunit/phpunit": "~4.0",
		"doctrine/dbal": "~2.3",
		"fuzz/rest-tester": "1.0.*"
	},
	"authors": [
		{
			"name": "Fuzz Productions",
			"email": "fuzzweb@fuzzproductions.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Fuzz\\Auth\\": "src/",
			"Fuzz\\Auth\\Tests\\": "tests/",
			"Fuzz\\Auth\\Tests\\Seeds\\": "tests/seeds"
		}
	},
	"scripts": {
		"test": [
			"vendor/bin/phpunit"
		],
		"test-coverage": [
			"vendor/bin/phpunit --coverage-html tests/coverage"
		],
		"open-coverage": [
			"open -a \"Google Chrome\" tests/coverage/index.html"
		]
	},
	"minimum-stability": "dev"
}
